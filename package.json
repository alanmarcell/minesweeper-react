{
  "name": "minesweeper-react",
  "version": "1.0.0",
  "description": "this is a Polutz React App.",
  "main": "dist/index.js",
  "typings": "src/index.ts",
  "scripts": {
    "lint": "tslint ./src/**/*.ts ./src/**/*.test.ts ./src/**/*.d.ts",
    "js": "rimraf dist && tsc --skipLibCheck",
    "babel": "babel dist-esnext --out-dir dist",
    "babel:w": " rimraf dist && babel -w dist-esnext -d dist --source-maps",
    "tsc:w": "rimraf dist-esnext && tsc -w --skipLibCheck",
    "pretest": "npm-run-all --parallel download-graphql-schema js",
    "mocha": "mocha ./dist/**/*.js --require babel-polyfill --compilers js:babel-register",
    "test": "nyc npm run mocha && nyc report --reporter=html --reporter=text-lcov > coverage.lcov && f() { EXIT=0; codecov --token=f1381414-f5ee-4356-b0b0-716d9e5abbe0 || exit $EXIT; }; f",
    "test:cover": "npm run test && opn ./coverage/index.html",
    "predebug": "npm run pretest",
    "debug": "node --nolazy --debug-brk=5858 dist/index.js",
    "docs:build": "typedoc --out ./docs ./ --includeDeclarations --excludeExternals --exclude '**/*.test.ts'",
    "docs": "npm run docs:build && opn ./docs/index.html",
    "prestart": "npm run pretest",
    "start": "npm run open:src",
    "test:watch": "npm run test -- --watch",
    "open:src": "babel-node tools/srcServer.js",
    "download-graphql-schema": "node tools/downloadGraphqlSchema.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanmarcell/minesweeper-react.git"
  },
  "author": "github.com/alanmarcell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alanmarcell/minesweeper-react/issues"
  },
  "homepage": "https://github.com/alanmarcell/minesweeper-react#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^8.0.29",
    "@types/react": "^16.0.5",
    "@types/react-relay": "^0.9.12",
    "@types/react-router": "^4.0.4",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-relay-plugin": "^0.11.0",
    "browser-sync": "^2.18.8",
    "codecov": "^2.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "opn-cli": "^3.1.0",
    "ptz-assert": "^1.6.3",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "tslint": "^5.0.0",
    "typedoc": "^0.6.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.8",
    "webpack": "~2.3.3",
    "webpack-bundle-analyzer": "~2.3.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fs": "^0.0.1-security",
    "graphql-relay": "^0.5.1",
    "prop-types": "^15.5.4",
    "ptz-core-domain": "^1.2.2",
    "ptz-log": "^1.0.0",
    "ptz-menu-domain": "^1.0.0",
    "ptz-user-domain": "^1.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-relay": "^0.10.0",
    "react-router": "^2.5.1",
    "react-router-relay": "^0.13.7",
    "request": "^2.81.0"
  }
}
